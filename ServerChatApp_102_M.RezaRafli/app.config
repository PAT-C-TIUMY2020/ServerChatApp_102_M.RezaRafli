<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
    <system.serviceModel>
        <behaviors>
            <endpointBehaviors>
                <behavior name="EndpointBehavior()">
                    <webHttp helpEnabled="true" />
                </behavior>
            </endpointBehaviors>
            <serviceBehaviors>
                <behavior name="MetadataBehavior()">
                    <serviceMetadata httpGetEnabled="true" httpsGetEnabled="false" />
                </behavior>
            </serviceBehaviors>
        </behaviors>
        <services>
            <service behaviorConfiguration="MetadataBehavior()" name="WCFService_2Way_102.ServiceCallback">
                <endpoint address="" binding="wsDualHttpBinding" bindingConfiguration=""
                          name="EndpointService" contract="WCFService_2Way_102.IServiceCallback" />
                <endpoint address="mex" binding="mexHttpBinding" bindingConfiguration=""
                          name="EndpointMex" contract="IMetadataExchange" />
                  <host>
                    <baseAddresses>
                        <add baseAddress="http://localhost:1907" />
                    </baseAddresses>
                </host>
            </service>
        </services>
    </system.serviceModel>
</configuration>